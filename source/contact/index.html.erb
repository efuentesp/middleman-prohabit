---
title: Contáctanos
---
<!-- Page Title -->
<div class="section section-breadcrumbs">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h1><%= current_page.data.title %></h1>
      </div>
    </div>
  </div>
</div>
<!-- End Page Title -->

<div class="section">
  <div class="container">
      <div class="row">
        <h2><%= data.contact.long_name %></h2>
        <div class="col-sm-7">
          <!-- Map -->
          <div id="contact-us-map">

          </div>
          <!-- End Map -->
        </div>
        <div class="col-sm-5">
          <!-- Contact Info -->
          <p class="contact-us-details">
            <b><i class="fa fa-map-marker"></i> Dirección:</b><br/>
            <%= data.contact.address1 %>. <%= data.contact.address2 %><br/>
            <%= data.contact.city %><br/><br/>
            <b><i class="fa fa-phone"></i> Oficina:</b> <%= data.contact.phone %><br/>
            <b><i class="fa fa-mobile"></i> Celular:</b> <%= data.contact.mobile %><br/>
            <b><i class="fa fa-whatsapp"></i> WhatsApp:</b> <%= data.contact.whatsapp %><br/>
            <b><i class="fa fa-envelope-o"></i> Email:</b> <%= mail_to data.contact.email, data.contact.email %>
          </p>
          <!-- End Contact Info -->
        </div>
      </div>
  </div>
</div>

<%= partial 'partials/calltoaction' %>
